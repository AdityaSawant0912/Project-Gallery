{
    //terminal
    "terminal.integrated.tabs.enabled": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.rightClickBehavior": "default",
    //C
    "C_Cpp.autocompleteAddParentheses": true,
    "C_Cpp.updateChannel": "Default",
    // "C_Cpp.vcFormat.newLine.beforeWhileInDoWhile": true,
    "C_Cpp.enhancedColorization": "Enabled",
    "C_Cpp.suggestSnippets": true,
    // "C_Cpp.vcFormat.space.afterComma": true,
    //code runner
    "code-runner.runInTerminal": true,
    //"code-runner.clearPreviousOutput": false,
    "code-runner.saveFileBeforeRun": true,
    //vs intelli
    "vsintellicode.java.completionsEnabled": true,
    //files
    "files.autoSave": "onFocusChange",
    "files.defaultLanguage": "${activeEditorLanguage}",
    "window.title": "${dirty}$(ツ)_/¯ {folderName}${dirty}",
    //"files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "*/.class": true,
        "*/.exe": true
    },
    //workbench
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.list.smoothScrolling": true,
    "workbench.sideBar.location": "right",
    "workbench.colorCustomizations": {
        "activityBar.foreground": "#12af98cc"
    },
    //"workbench.editor.enablePreview": false,
    //editor
    "editor.cursorBlinking": "expand",
    "editor.fontSize": 14,
    "editor.formatOnType": true,
    "editor.fontLigatures": true,
    "editor.cursorStyle": "line-thin",
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": true,
    "editor.bracketPairColorization.enabled": false,
    //"editor.dragAndDrop": false,
    "editor.suggest.showStatusBar": false,
    "editor.snippetSuggestions": "inline",
    "editor.suggest.insertMode": "insert",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.autoClosingBrackets": "languageDefined",
    "editor.autoClosingDelete": "auto",
    "editor.minimap.enabled": false,
    "editor.autoIndent": "advanced",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "editor.fontFamily": "Fira Code, 'Cascadia Mono PL', Cascadia Mono PL",
    "editor.fontWeight": "normal",
    "problems.decorations.enabled": false,
    "editor.guides.bracketPairs": true,
    "editor.guides.indentation": true,
    "editor.guides.bracketPairsHorizontal": true,
    //explorer
    "explorer.sortOrder": "type",
    //theme
    //system
    // "workbench.iconTheme": "office-material-icon-theme",
    "extensions.supportUntrustedWorkspaces": {
        "ms-vsliveshare.vsliveshare": {
            "supported": true,
            "version": "1.0.4836"
        },
        "ms-vsliveshare.vsliveshare-audio": {
            "supported": true,
            "version": "0.1.91"
        },
    },
    "update.enableWindowsBackgroundUpdates": false,
    "terminal.integrated.gpuAcceleration": "off",
    "glassit.alpha": 215,
    "explorer.confirmDelete": false,
    "java.project.sourcePaths": [
        "C:\\Expt 6\\org\\Calculator,"
    ],
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "C:\\Program Files\\Java\\jdk-16.0.2",
            "default": true
        }
    ],
    "java.server.launchMode": "LightWeight",
    "workbench.colorTheme": "Ayu Mirage",
}